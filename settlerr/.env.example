# AWS Cognito Configuration
# Copy this file to .env.local and fill in your AWS Cognito details

# Get these values from AWS Console -> Cognito -> Your User Pool
REACT_APP_AWS_REGION=us-east-1
REACT_APP_USER_POOL_ID=us-east-1_XXXXXXXXX
REACT_APP_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Set to false when you have AWS credentials configured
REACT_APP_USE_DEMO_AUTH=true